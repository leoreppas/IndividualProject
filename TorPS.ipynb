    "exit_nodes_low = 0\n",
    "\n",
    "\n",
    "class Relays():\n",
    "    '''\n",
    "    This class deals with the creation of the three relay types\n",
    "    and their three varying bandwidth clacifications\n",
    "    '''\n",
    "    \n",
    "    def create_entry_nodes(self, number1, number2, number3):\n",
    "        global entry_nodes_high, entry_nodes_med, entry_nodes_low\n",
    "        entry_nodes_high += number1\n",
    "        entry_nodes_med += number2\n",
    "        entry_nodes_low += number3\n",
    "        \n",
    "    def create_middle_nodes(self, number1, number2, number3):\n",
    "        global middle_nodes_high, middle_nodes_med, middle_nodes_low\n",
    "        middle_nodes_high += number1\n",
    "        middle_nodes_med += number2\n",
    "        middle_nodes_low += number3\n",
    "        \n",
    "    def create_exit_nodes(self, number1, number2, number3):\n",
    "        global exit_nodes_high, exit_nodes_med, exit_nodes_low\n",
    "        exit_nodes_high += number1\n",
    "        exit_nodes_med += number2\n",
    "        exit_nodes_low += number3\n",
    "        \n",
    "    def print_nodes(self):\n",
    "        global entry_nodes_high, middle_nodes_high, exit_nodes_high\n",
    "        global entry_nodes_med, middle_nodes_med, exit_nodes_med\n",
    "        global entry_nodes_low, middle_nodes_low, exit_nodes_low\n",
    "        \n",
    "        entry_nodes = entry_nodes_high + entry_nodes_med + entry_nodes_low\n",
    "        middle_nodes = middle_nodes_high + middle_nodes_med + middle_nodes_low\n",
    "        exit_nodes = exit_nodes_high + exit_nodes_med + exit_nodes_low\n",
    "        \n",
    "        high_bandwidth = entry_nodes_high + middle_nodes_high + exit_nodes_high\n",
    "        med_bandwidth = entry_nodes_med + middle_nodes_med + exit_nodes_med\n",
    "        low_bandwidth = entry_nodes_low + middle_nodes_low + exit_nodes_low\n",
    "        \n",
    "        print(\"\"\"\n",
    "        Number of entry nodes: {}\n",
    "        Number of middle nodes: {}\n",
    "        Number of exit nodes: {}\\n\n",
    "        Number of high bandwidth nodes: {}\n",
    "        Number of medium bandwidth nodes: {}\n",
    "        Number of low bandwidth nodes: {}\n",
    "        \"\"\". format(entry_nodes, middle_nodes, exit_nodes, high_bandwidth, med_bandwidth, low_bandwidth))\n",
    "            \n",
